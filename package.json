{
    "name": "npm-package-base",
    "description": "A base project from which to start any kind of npm package.",
    "version": "0.0.1",
    "private": true,
    "keywords": [
        "start",
        "template"
    ],
    "repository": { 
        "type": "git",
        "url": "https://github.com/Eagerod/npm-package-base.git"
    },
    "dependencies": {
    },
    "devDependencies": {
        "jsdoc": "3.3.2",
        "eslint": "0.24.1",
        "nodeunit": "0.9.1",
        "istanbul": "0.3.17"
    },
    "author":{
        "name": "Aleem Haji",
        "author": "hajial@gmail.com"
    },
    "scripts": {
        "test": "./node_modules/.bin/nodeunit tests/test.js",
        "docs": "./node_modules/.bin/jsdoc --recurse ./lib -d docs",
        "lint": "./node_modules/.bin/eslint .",
        "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/nodeunit tests/test.js"
    }
}
